{
  "name": "mamascota-proxy",
  "type": "module",
  "scripts": {
    "check-agent": "node check-agent-access.mjs",
    "start:proxy": "node server.mjs",
    "start": "tsx server.mjs",
    "build:proxy": "tsc --build tsconfig.proxy.json",
    "proxy:start": "npm run build:proxy && node server.mjs",
    "proxy:dev": "nodemon server.mjs",
    "proxy:clear": "node --env-file .env server.mjs",
    "agent:test": "curl -X POST http://localhost:3001/agent -H 'Content-Type: application/json' -d '{\"message\":\"\",\"pet\":{\"id\":\"test001\",\"name\":\"Ася\",\"species\":\"cat\",\"sex\":\"female\",\"ageYears\":15,\"neutered\":false},\"symptomKeys\":[\"drinking_too_much\",\"frequent_urination\"]}'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "tsx": "^4.20.6"
  }
}
